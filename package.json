{
  "name": "matrix-neoboard-standalone",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:https": "VITE_DEV_SSL=true vite",
    "preview": "vite preview",
    "preview:https": "VITE_DEV_SSL=true vite preview",
    "build": "tsc && vite build",
    "test": "jest",
    "test:all": "jest --coverage --watchAll=false",
    "lint": "eslint . --max-warnings 0",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install",
    "depcheck": "depcheck --ignores=@babel/plugin-proposal-private-property-in-object,@babel/plugin-transform-private-property-in-object,@changesets/cli,@swc/core,@swc/jest,@testing-library/dom,@testing-library/jest-dom,@testing-library/react,@testing-library/react-hooks,@types/jest,@types/jest-axe,@typescript-eslint/eslint-plugin,@typescript-eslint/parser,jest-axe,jest-environment-jsdom,prettier-plugin-organize-imports",
    "deduplicate": "yarn-deduplicate",
    "translate": "i18next \"src/**/*.{ts,tsx}\"",
    "generate-disclaimer": "cp LICENSE ./build/LICENSE.txt && cp NOTICE ./build/NOTICE.txt && yarn licenses generate-disclaimer --prod >> ./build/NOTICE.txt && yarn licenses list --prod --json --no-progress > ./build/licenses.json"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": "^1.22.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "@changesets/cli": "^2.27.1",
    "@swc/core": "^1.4.8",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^27.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "i18next": "^23.10.1",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^2.5.0",
    "i18next-parser": "^8.13.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react-i18next": "^14.1.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nordeck/matrix-neoboard.git"
  },
  "bugs": {
    "url": "https://github.com/nordeck/matrix-neoboard/issues"
  }
}
